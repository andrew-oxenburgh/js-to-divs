{
  "name": "js-to-divs",
  "version": "1.6.5",
  "description": "Given a js object, output a tree of divs that reflect it",
  "keywords": [
    "js",
    "json",
    "transform",
    "div",
    "formatting",
    "browse"
  ],
  "main": "index.js",
  "scripts": {
    "test-both": "npm run test && npm run test-with-report && gsed -e '/^...js$/,+3d' SPECIFICATION.md -i",
    "test": "mocha .",
    "test-with-report": "mocha . --reporter markdown > SPECIFICATION.md",
    "qa": "rm -rf ./node_modules && npm i && npm test && jscs . && npm run test-with-report"
  },
  "license": "MIT",
  "devDependencies": {
    "mocha": "^3.2.0",
    "should": "^11.1.2"
  },
  "author": {
    "name": "Andrew Oxenburgh",
    "email": "andrew.oxenburgh@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/andrew-oxenburgh/js-to-divs/issues",
    "email": "andrew.oxenburgh@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrew-oxenburgh/js-to-divs.git"
  },
  "dependencies": {
    "xml-formatter": "^1.0.1"
  }
}
